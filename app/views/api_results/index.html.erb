<p id="notice"><%= notice %></p>

<h1>Api Results</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_results.each do |api_result| %>
      <tr>
        <td><%= link_to 'Show', api_result %></td>
        <td><%= link_to 'Edit', edit_api_result_path(api_result) %></td>
        <td><%= link_to 'Destroy', api_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api Result', new_api_result_path %>
